def line(a,b):
    if len(b)>1:
        b = b[0]
    elif b == "":
        b = "*"

    print(b*10)
# You can test your function by calling it within the following block
if __name__ == "__main__":
    line(10, "")
def box_of_hashes(height):
    i = 0
    while i<height:
        line (height,"#")
        i += 1


# You can test your function by calling it within the following block
if __name__ == "__main__":
    box_of_hashes(5)
